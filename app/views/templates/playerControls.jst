<% if (albumArt) { %>
    <img class="album" src=<%= albumArt %>></img>
<% } else { %>
    <button class="album">
        <svg>
            <use xlink:href="<%= require('../../assets/icons/music-lg.svg') %>"/>
        </svg>
    </button>
<% }%>
<button id="toggle-expand">
    <svg>
        <use class="up" xlink:href="<%= require('../../assets/icons/arrow-up-sm.svg') %>"/>
        <use class="down" xlink:href="<%= require('../../assets/icons/arrow-down-sm.svg') %>"/>
    </svg>
</button>

<button id="prev">
    <svg>
        <use xlink:href="<%= require('../../assets/icons/prev-lg.svg') %>"/>
    </svg>
</button>
<button id="play">
    <svg>
        <% if (isPlaying) { %>
            <use xlink:href="<%= require('../../assets/icons/pause-lg.svg') %>"/>
        <% } else { %>
            <use xlink:href="<%= require('../../assets/icons/play-lg.svg') %>"/>
        <% }%>
    </svg>
</button>
<button id="next">
    <svg>
        <use xlink:href="<%= require('../../assets/icons/next-lg.svg') %>"/>
    </svg>
</button>

